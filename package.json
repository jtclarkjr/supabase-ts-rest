{
  "name": "@jtclarkjr/supabase-ts-rest",
  "version": "0.2.4",
  "description": "A lightweight TypeScript client for Supabase REST API with authentication support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "bun run tsc",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "lint": "oxlint src/**/*.ts",
    "lint:fix": "oxlint src/**/*.ts --fix",
    "prepublishOnly": "bun run build",
    "dev": "bun run build --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "supabase",
    "rest",
    "api",
    "client",
    "typescript",
    "authentication",
    "database"
  ],
  "author": "jtclarkjr",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jtclarkjr/supabase-ts-rest.git",
    "directory": "npm"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@types/bun": "^1.2.20",
    "oxlint": "^1.11.2",
    "prettier": "^3.6.2",
    "typescript": "^5.9.0"
  },
  "dependencies": {},
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
